{
  "name": "midweste/cloudflare-spf-flattener",
  "description": "A library to flatten Cloudflare records via Cloudflare API",
  "type": "library",
  "keywords": [
    "spf",
    "flattener",
    "dns"
  ],
  "version": "0.1.0",
  "homepage": "https://github.com/midweste/cloudflare-spf-flattener",
  "license": "MIT",
  "authors": [
    {
      "name": "midweste",
      "email": "midweste@devbunker.com",
      "homepage": "https://github.com/midweste",
      "role": "Developer"
    }
  ],
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "CloudflareSpf\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "midweste/spf-lib-flattener": "dev-main",
    "cloudflare/sdk": "^1.0",
    "symfony/console": "^6.0",
    "psr/log": "^3.0",
    "phpmailer/phpmailer": "^6.0"
  },
  "require-dev": {
    "kint-php/kint": "^3.0"
  },
  "minimum-stability": "dev"
}
